{
  "name": "project-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "run-s clean build",
    "start": "npm run docker:up",
    "dev": "run-p build:watch serve:watch",
    "docker:up": "docker-compose up",
    "docker:up:prod": "docker-compose -f docker-compose.prod.yml up",
    "docker:build": "docker-compose build",
    "build": "NODE_ENV=production webpack --progress",
    "build:watch": "NODE_ENV=development webpack --progress -w",
    "serve": "node dist/server.js",
    "serve:watch": "nodemon dist/server.js",
    "lint": "run-s lint:css lint:js",
    "lint:css": "stylelint 'src/scss/**/*.scss' --color --formatter verbose",
    "lint:js": "eslint . --ext .js,.ts",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "css-loader": "^6.6.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.5.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "stylelint": "^14.5.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "express": "^4.17.2"
  }
}
